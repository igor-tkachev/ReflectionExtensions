<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<TargetFrameworks>net462;net461;net46;net452;net451;net45;net40;net35;net30;net20;netcoreapp1.0;netcoreapp1.1;netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;netstandard1.0;netstandard1.1;netstandard1.2;netstandard1.3;netstandard1.4;netstandard1.5;netstandard1.6;netstandard2.0</TargetFrameworks>
		<LangVersion>preview</LangVersion>
		<NullableContextOptions>enable</NullableContextOptions>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\Build\Jetbrains.Annotations.cs">
			<Link>Properties\Jetbrains.Annotations.cs</Link>
		</Compile>
	</ItemGroup>


	<ItemGroup>
		<Compile Remove="**/*.net20.*" />
	</ItemGroup>
	<ItemGroup>
		<Compile Remove="**/*.net30.*" />
	</ItemGroup>

	<ItemGroup Condition=" '$(TargetFramework)'=='net20' ">
		<Compile Include="**/*.net20.*" />
	</ItemGroup>

	<ItemGroup Condition=" '$(TargetFramework)'=='net30' ">
		<Compile Include="**/*.net30.*" />
	</ItemGroup>
</Project>
